{
  "name": "habix",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "digix-ui": "^2.9.0",
    "font-awesome": "^4.7.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-scripts": "3.1.2",
    "redux": "^4.0.4",
    "sass": "^1.23.0"
  },
  "scripts": {
    "start": "set PORT=2000 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sass": "sass --watch src/styles/scss:src/styles/css"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.2"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
